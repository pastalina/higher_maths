        "import numpy as np\n",
        "import scipy\n",
        "import scipy.linalg\n",
        "\n",
        "A = np.array([[ 1, 2,  3],\n",
        "              [ 2, 16, 21],\n",
        "              [ 4, 28, 73]])\n",
        "P, L, U = scipy.linalg.lu(A)\n",
        "print(A)\n",
        "print(P)\n",
        "print(L)\n",
        "print(U)\n",
        "print(np.dot(np.dot(P, L), U))"
